  sub abreveacute from [abreveacute.alt];
  sub abrevedotbelow from [abrevedotbelow.alt];
  sub abrevegrave from [abrevegrave.alt];
  sub abrevehoi from [abrevehoi.alt];
  sub abrevetilde from [abrevetilde.alt];
  sub acircumflex from [acircumflex.alt];
  sub acircumflexacute from [acircumflexacute.alt];
  sub acircumflexdotbelow from [acircumflexdotbelow.alt];
  sub acircumflexgrave from [acircumflexgrave.alt];
  sub acircumflexhoi from [acircumflexhoi.alt];
  sub acircumflextilde from [acircumflextilde.alt];
  sub adblgrave from [adblgrave.alt];
  sub adieresis from [adieresis.alt];
  sub adotbelow from [adotbelow.alt];
  sub agrave from [agrave.alt];
  sub ahoi from [ahoi.alt];
  sub ainvertedbreve from [ainvertedbreve.alt];
  sub amacron from [amacron.alt];
  sub aogonek from [aogonek.alt];
  sub aring from [aring.alt];
  sub aringacute from [aringacute.alt];
  sub atilde from [atilde.alt];
#< feature
} salt;
feature case {
# Case Sensitive Forms

#> feature
  sub breveacute by breveacute.case;
  sub brevegrave by brevegrave.case;
  sub brevehoi by brevehoi.case;
  sub brevetilde by brevetilde.case;
  sub circumflexacute by circumflexacute.case;
  sub circumflexgrave by circumflexgrave.case;
  sub circumflexhoi by circumflexhoi.case;
  sub circumflextilde by circumflextilde.case;
  sub ringacute by ringacute.case;
#< feature
} case;
